{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "persistence": {
      "type": "library",
      "root": "libs/persistence",
      "entryFile": "index",
      "sourceRoot": "libs/persistence/src",
      "compilerOptions": {
        "tsConfigPath": "libs/persistence/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "logger": {
      "type": "library",
      "root": "libs/logger",
      "entryFile": "index",
      "sourceRoot": "libs/logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger/tsconfig.lib.json"
      }
    },
    "sdk-gateway-dto": {
      "type": "library",
      "root": "libs/sdk-gateway-dto",
      "entryFile": "index",
      "sourceRoot": "libs/sdk-gateway-dto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sdk-gateway-dto/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "event-broker": {
      "type": "library",
      "root": "libs/event-broker",
      "entryFile": "index",
      "sourceRoot": "libs/event-broker/src",
      "compilerOptions": {
        "tsConfigPath": "libs/event-broker/tsconfig.lib.json"
      }
    },
    "healthcheck": {
      "type": "library",
      "root": "libs/healthcheck",
      "entryFile": "index",
      "sourceRoot": "libs/healthcheck/src",
      "compilerOptions": {
        "tsConfigPath": "libs/healthcheck/tsconfig.lib.json"
      }
    }
  }
}