{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/persistence/(.*)": "<rootDir>/../libs/persistence/src/$1",
    "@app/persistence": "<rootDir>/../libs/persistence/src",
    "@app/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
    "@app/utils": "<rootDir>/../libs/utils/src",
    "@app/logger/(.*)": "<rootDir>/../libs/logger/src/$1",
    "@app/logger": "<rootDir>/../libs/logger/src",
    "libs/sdk-gateway-dto/(.*)": "<rootDir>/../libs/sdk-gateway-dto/src/$1",
    "libs/sdk-gateway-dto": "<rootDir>/../libs/sdk-gateway-dto/src",
    "libs/redis/(.*)": "<rootDir>/../libs/redis/src/$1",
    "libs/redis": "<rootDir>/../libs/redis/src"
  }
}